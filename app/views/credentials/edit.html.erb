<%= turbo_frame_tag "credential_collection_#{@credential.collection_id}" do %>

  <div class="mx-auto md:w-2/3 w-full">
  <h1 class="font-bold text-3xl">Editing credential</h1>

  <%= render "form", credential: @credential, collection: @credential.collection_id %>

    <%= link_to "cancel", @credential.collection, class: "ml-2 rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
  </div>
<% end %>