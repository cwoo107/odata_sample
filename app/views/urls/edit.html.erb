<%= turbo_frame_tag "url_collection_#{@url.collection_id}" do %>
<div class="mx-auto md:w-2/3 w-full">
  <h1 class="font-bold text-4xl">Editing url</h1>

  <%= render "form", url: @url, collection: @url.collection_id %>

  <%= link_to "cancel", @url.collection, class: "rounded-md bg-white px-2.5 py-1.5 text-sm font-semibold text-gray-900 shadow-sm ring-1 ring-inset ring-gray-300 hover:bg-gray-50" %>
</div>
<% end %>