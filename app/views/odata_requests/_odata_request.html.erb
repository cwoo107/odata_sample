<div id="<%= dom_id odata_request %>">
  <div  class="flex items-center justify-between gap-x-6 py-5">
    <div class="min-w-0 w-2/3">
      <div class="flex items-start gap-x-3">
        <p class="text-sm font-semibold leading-6 text-gray-900"><%= odata_request.name %></p>
      </div>
      <div class="mt-1 flex items-center gap-x-2 text-xs leading-5 text-gray-500">
        <p class="whitespace-nowrap">Last updated <time datetime=""><%= odata_request.updated_at.strftime('%m/%d/%y %I:%M %p') %></time></p>
      </div>
    </div>
  </div>
  <%= render partial: 'odata_requests/code_blocks', locals: { odata_request: odata_request} %>
</div>
