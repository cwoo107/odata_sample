<%= turbo_frame_tag dom_id @odata_request do %>

  <div class="mx-auto md:w-2/3 w-full">
  <h1 class="font-bold text-4xl">Editing odata request</h1>

  <%= render "form", odata_request: @odata_request, collection: @odata_request.collection_id %>

  <%= link_to "Cancel", @odata_request, class: "ml-2 rounded-lg py-3 px-5 bg-gray-100 inline-block font-medium" %>
</div>
<% end %>